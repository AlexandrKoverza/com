{
	"name": "gulp-2022",
	"version": "1.0.0",
	"description": "",
	"main": "gulpfile.js",
	"type": "module",
	"scripts": {
		"dev": "gulp",
		"validate:html": "java -jar ./node_modules/vnu-jar/build/dist/vnu.jar --html dist/*.html",
		"validate:css": "java -jar ./node_modules/vnu-jar/build/dist/vnu.jar --css dist/*.css",
		"validate:js": "eslint src/js/app.js",
		"validate:lighthouse-mobile": "lighthouse http://localhost:3000 --output html --output-path ./lighthouse-mobile.html --view --chrome-flags=\"--headless\"",
		"validate:lighthouse-desktop": "lighthouse http://localhost:3000 --output html --output-path ./lighthouse-desktop.html --view --preset=desktop --chrome-flags=\"--headless\"",
		"validate:lighthouse-both": "yarn yarn validate:lighthouse-desktop && validate:lighthouse-mobile",
		"build": "gulp build --build",
		"zip": "gulp deployZIP --build",
		"deploy": "gulp deployFTP --build",
		"svgSprive": "gulp svgSpriteTask",
		"server": "json-server --watch db.json -m ./node_modules/json-server-auth",
		"sonar": "sonarqube-scanner"
	},
	"author": "FLS",
	"license": "ISC",
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"air-datepicker": "^3.4.0",
		"animejs": "^3.2.1",
		"date-fns": "^2.30.0",
		"gulp-cli": "^2.3.0",
		"inputmask": "^5.0.8",
		"jquery": "^3.7.0",
		"lg-fullscreen": "^1.2.1",
		"lg-thumbnail": "^1.2.1",
		"lg-zoom": "^1.3.0",
		"lightgallery": "^2.7.2",
		"paginationjs": "^2.6.0",
		"typed.js": "^2.0.16"
	},
	"devDependencies": {
		"@eslint/js": "^9.12.0",
		"@fortawesome/fontawesome-free": "latest",
		"browser-sync": "latest",
		"chart.js": "latest",
		"del": "latest",
		"eslint": "^9.12.0",
		"globals": "^15.11.0",
		"gulp": "4.0.2",
		"gulp-autoprefixer": "latest",
		"gulp-clean-css": "latest",
